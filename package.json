{
  "name": "@gscd/a11y-engine",
  "version": "1.0.2",
  "description": "A11yMARKET's Accessibility Library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "BlueNyang",
  "license": "MIT",
  "homepage": "https://github.com/Gamesung-Coding/a11y-engine#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gamesung-Coding/a11y-engine.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "contributors": [
    {
      "name": "Gamesung Coding",
      "githubUsername": "Gamesung-Coding",
      "url": "https://github.com/Gamesung-Coding"
    },
    {
      "name": "Sumin Kim",
      "githubUsername": "Gravity251",
      "url": "https://github.com/Gravity251"
    },
    {
      "name": "GyuTae Ahn",
      "githubUsername": "BlueNyang",
      "url": "https://github.com/BlueNyang"
    }
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.mjs",
      "require": "./dist/react.cjs",
      "types": "./dist/types/adapters/react.d.ts"
    },
    "./vue": {
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.cjs",
      "types": "./dist/types/adapters/vue.d.ts"
    },
    "./css": "./dist/a11y.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "dev": "rollup -c rollup.config.mjs -w"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/react": "^19.2.10",
    "rollup": "^4.57.1",
    "rollup-plugin-dts": "^6.3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "accessibility",
    "a11y",
    "library"
  ],
  "peerDependencies": {
    "react": "^19.2.4",
    "vue": "^3.5.27"
  }
}
